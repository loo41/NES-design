{
  "name": "nes-design",
  "version": "0.1.0",
  "description": "React Game Style UI Component Library",
  "main": "lib/index.js",
  "module": "es/index.js",
  "unpkg": "dist/nes-design.min.js",
  "typings": "types/index.d.ts",
  "scripts": {
    "test": "jest",
    "docz:dev": "rm -rf .docz && mkdir .docz && cp -r static .docz/static && docz dev ",
    "docz:build": "docz build",
    "docz:serve": "docz build && docz serve",
    "docz:clean": "rm -rf .docz && rm -rf docs && gatsby clean",
    "build": "gulp",
    "build:components": "gulp compile",
    "build:types": "tsc --declaration --emitDeclarationOnly",
    "lint": "eslint components --fix --ext .ts,.tsx ",
    "pre-commit": "lint-staged"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "{components,theme}/**/*.{tsx,ts,jsx,js}": [
      "eslint --fix"
    ]
  },
  "author": "TCYong",
  "license": "MIT",
  "homepage": "https://loo41.github.io/NES-design/",
  "warehouse": "https://github.com/loo41/NES-design",
  "dependencies": {
    "classnames": "^2.3.1",
    "nes.css": ">=2.3.0",
    "rc-table": "^7.14.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.13.16",
    "@babel/core": "^7.14.0",
    "@babel/plugin-transform-runtime": "^7.13.15",
    "@babel/preset-env": "^7.14.1",
    "@babel/preset-typescript": "^7.13.0",
    "@types/enzyme": "^3.10.8",
    "@types/jest": "^26.0.23",
    "@types/sinon": "^10.0.0",
    "@typescript-eslint/parser": "^4.22.1",
    "docz": "^2.3.1",
    "enzyme": "^3.11.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "gatsby-plugin-sass": "^3.2.0",
    "gulp": "^4.0.2",
    "gulp-autoprefixer": "^7.0.1",
    "gulp-babel": "^8.0.0",
    "gulp-cli": "^2.3.0",
    "gulp-cssnano": "^2.1.3",
    "gulp-eslint": "^6.0.0",
    "gulp-rename": "^2.0.0",
    "gulp-sass": "^4.1.0",
    "husky": "^2.7.0",
    "jest": "^26.6.3",
    "lint-staged": "^11.0.0",
    "node-sass": "^4.14.1",
    "prettier": "^1.19.1",
    "prettier-eslint": "^12.0.0",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "react-highlight": "^0.14.0",
    "sass-resources-loader": "^2.2.1",
    "sinon": "^10.0.0",
    "typescript": "^4.2.4"
  },
  "peerDependencies": {
    "nes.css": ">=2.3.0",
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0"
  },
  "sideEffects": [
    "dist/*",
    "esm/*.css",
    "lib/*.css",
    "*.scss"
  ],
  "jest": {
    "moduleFileExtensions": [
      "js",
      "jsx",
      "json",
      "ts",
      "tsx"
    ],
    "transformIgnorePatterns": [
      "node_modules"
    ]
  }
}
